{
  "icon": "icon.png",
  "name": "dtfvs",
  "displayName": "Duck Themes for VS",
  "description": "Duck Themes is a customization for your VS that changes the icons and adds new themes to make your development environment a more comfortable place.",
  "version": "1.0.0",
  "publisher": "patoguisadok",
  "repository": {
  "type": "git",
  "url": "https://github.com/patoguisadok/Duck-Themes-for-VS"
},
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "iconThemes": [
      {
        "id": "duck-icon-animated",
        "label": "The best custom icons",
        "path": "./icons/icons.json"
      }
    ],
    "themes": [
      {
        "label": "Ocean",
        "uiTheme": "vs-dark",
        "path": "./themes/OceanTheme.json"
      },
      {
        "label": "Low-Night",
        "uiTheme":"vs-dark",
        "path": "./themes/LowNightTheme.json"
      },
      {
        "label": "High-Night",
        "uiTheme": "vs-dark",
        "path": "./themes/HighNightTheme.json"
      },
      {
        "label": "Beach",
        "uiTheme": "hc-light",
        "path": "./themes/BeachTheme.json"
      },
      {
        "label": "Sunset",
        "uiTheme": "vs-dark",
        "path": "./themes/SunsetTheme.json"
      },
      {
        "label": "Sunset Vista",
        "uiTheme": "vs-dark",
        "path": "./themes/SunsetVistaTheme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "tsc -w npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "tsc",
    "start": "node ./dist/extension.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  }
}
