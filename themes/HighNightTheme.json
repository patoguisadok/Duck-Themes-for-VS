{
    "name": "High-Night",
    "colors": {
        "editor.background": "#121212",
        "editor.foreground": "#FFFFFF",
        "editorLineNumber.foreground": "#FF4081",
        "editorCursor.foreground": "#FFD700",
        "editor.selectionBackground": "#333333",
        "editor.wordHighlightBackground": "#555555",
        "editor.keywordColor": "#FF5722",
        "editor.stringColor": "#00E676",
        "editor.commentColor": "#B0BEC5",
        "activityBar.background": "#1E1E1E",
        "sideBar.background": "#121212",
        "statusBar.background": "#1E1E1E"
    },

    "tokenColors": [
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#0277BD"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#388E3C"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6D4C41",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#BF360C"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "number",
            "settings": {
                "foreground": "#7B1FA2"
            }
        }
    ]
}
